              {{ block.settings.side_panel_1_content }}
              {{ block.settings.side_panel_1_page.content }}
            </div>
          {%- endif -%}
          {%- if block.settings.side_panel_2_label != '' -%}
            <div class="side-panel-content--tab-panel rte">
