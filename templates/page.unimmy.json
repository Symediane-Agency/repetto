          }
        },
        "155bd3bf-99bc-441c-a6d6-d4d0fee86c2f": {
          "type": "columnItem",
          "settings": {
            "cardItemTitle": "<p><strong>Objet de toutes les convoitises, La Maison séduit une clientèle raffinée, composée à la fois de danseurs et d’amateurs de cet univers unique.<\/strong><\/p>",
            "cardItemImage": "shopify:\/\/shop_images\/Rectangle_258.png",
            "imagePosition": "top",
            "titlePosition": "bottom"
